<%- include('../v_layout/head') %>

<body class="d-flex align-items-center py-4 bg-body-tertiary">
    <link rel="stylesheet" href="/css-tambahan/layout-login.css">
    <main class="form-signin w-100 m-auto">
        <form method="post" action="/auth/proses-login">
          <img class="mb-4" src="/gambar/gambar.png" alt="" width="120" height="90">
          <h1 class="h3 mb-3 fw-normal">Silahkan masuk</h1>
      
          <div class="form-floating">
            <input type="email" class="form-control" name="form_email" placeholder="name@example.com">
            <label for="floatingInput">Alamat Email</label>
          </div>
          <div class="form-floating">
            <input type="password" class="form-control" name="form_password" placeholder="Password">
            <label for="floatingPassword">Kata Sandi</label>
          </div>
          <button class="btn btn-primary w-100 py-2" type="submit">Masuk</button>
          <p class="mt-5 mb-3 text-body-secondary">&copy; 2017â€“2024</p>
        </form>
      </main>


</body>
